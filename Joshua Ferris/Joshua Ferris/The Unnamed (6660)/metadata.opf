        <dc:title>The Unnamed</dc:title>
        <dc:creator opf:file-as="Ferris, Joshua" opf:role="aut">Joshua Ferris</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
