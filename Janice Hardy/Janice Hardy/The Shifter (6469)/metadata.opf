        <dc:title>The Shifter</dc:title>
        <dc:creator opf:file-as="Hardy, Janice" opf:role="aut">Janice Hardy</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
