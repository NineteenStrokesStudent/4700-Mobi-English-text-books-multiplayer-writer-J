        <dc:title>Chasing Silver</dc:title>
        <dc:creator opf:file-as="Craig, Jamie" opf:role="aut">Jamie Craig</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
