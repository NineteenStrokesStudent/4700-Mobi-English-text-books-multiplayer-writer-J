        <dc:title>East of the Sun</dc:title>
        <dc:creator opf:file-as="Gregson, Julia" opf:role="aut">Julia Gregson</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
