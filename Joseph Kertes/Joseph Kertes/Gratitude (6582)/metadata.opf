        <dc:title>Gratitude</dc:title>
        <dc:creator opf:file-as="Kertes, Joseph" opf:role="aut">Joseph Kertes</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
